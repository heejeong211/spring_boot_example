<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.ChartsMapper">

	<!-- 리스트 출력 -->
	<select id="selectChartList" resultType="com.example.demo.vo.BarToppingVO">
		SELECT * FROM BAR_TOPPING
	</select>
	
	<insert id="insertChart">
		INSERT INTO BAR_TOPPING (NAME, NUM) VALUES (#{name}, #{num})
	</insert>
	
</mapper>